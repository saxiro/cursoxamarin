<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1Vagas.Paginas.DetalhesVagas">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding NomeVaga}" />
            <Label Text="{Binding Empresa}" />
            <Label Text="{Binding Quantidade}" />
            <Label Text="{Binding Cidade}" />
            <Label Text="{Binding Salario}" />
            <Label Text="{Binding Tipo}" />
            <Label Text="{Binding Telefone}" />
            <Label Text="{Binding Email}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>